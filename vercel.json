{
    "version": 2,
    "builds": [
        {
            "src": "src/shared/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/register",
            "dest": "src/routes/index.routes.ts"
        },
        {
            "src": "/api/users",
            "dest": "src/routes/index.routes.ts"
        },
        {
            "src": "/(.*)",
            "dest": "src/shared/server.ts"
        }
    ]
}
